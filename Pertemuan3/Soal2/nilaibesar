#include <iostream> 
using namespace std;

int main(){
    int a, b, c, d;
    
    cout<<"Input bilangan pertama: ";
    cin>>a;
    cout<<"Input bilangan kedua: ";
    cin>>b;
    cout<<"Input bilangan ketiga: ";
    cin>>c;
    
    if (a>=b && a>=c){
        d = a;}
    else if (b >= a && b >= c){
        d = b;}
    else {
        d = c;}
    
    cout<<"Bilangan terbesar adalah: "<< d << endl;

return 0;
}
